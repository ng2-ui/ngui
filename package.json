{
  "name": "ng2-ui",
  "version": "0.12.2",
  "description": "Collection of Quality Angular2 User Interfaces",
  "license": "MIT",
  "main": "dist/ng2-ui.umd.js",
  "module": "dist/index.js",
  "typings": "dist/index.d.ts",
  "scripts": {
    "start": "NODE_ENV=dev webpack-dev-server --quiet --port 9001 --content-base app --config app/webpack.config --open",
    "lint": "tslint 'src/**/*.ts' 'app/**/*.ts'",
    "clean": "rimraf dist",
    "build": "npm-run-all --serial clean build:ngc build:umd build:app",
    "build:ngc": "ngc -p tsconfig.ngc.json",
    "build:umd": "NODE_ENV=prod webpack",
    "build:app": "NODE_ENV=prod webpack --config app/webpack.config",
    "upgrade": "npm-check-updates --upgradeAll"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ng2-ui/ng2-ui.git"
  },
  "author": "Allen Kim<allenhwkim@gmail.com>",
  "bugs": {
    "url": "https://github.com/ng2-ui/ng2-ui/issues"
  },
  "homepage": "https://github.com/ng2-ui/ng2-ui#readme",
  "keywords": [
    "angular2",
    "auto-complete",
    "datetime-picker",
    "google-maps",
    "input",
    "select",
    "popup",
    "modal",
    "tooltip",
    "menu",
    "overlay",
    "scrollable"
  ],
  "peerDependencies": {
    "ng2-auto-complete": "0.8.2",
    "ng2-collapsable": "^0.4.0",
    "ng2-datetime-picker": "0.12.0",
    "ng2-infinite-list": "0.5.1",
    "ng2-map": "0.12.2",
    "ng2-menu": "0.5.1",
    "ng2-overlay": "0.7.0",
    "ng2-parallax-scroll": "0.4.0",
    "ng2-popup": "0.4.0",
    "ng2-scrollable": "0.7.0",
    "ng2-sticky": "0.4.0",
    "ng2-tab": "0.4.0",
    "ng2-tooltip-overlay": "0.4.0",
    "ng2-ui-sortable": "0.3.0"
  },
  "devDependencies": {
    "@angular/common": "^2.3.0",
    "@angular/compiler": "^2.3.0",
    "@angular/compiler-cli": "^2.3.0",
    "@angular/core": "^2.3.0",
    "@angular/forms": "^2.3.0",
    "@angular/http": "^2.3.0",
    "@angular/platform-browser": "^2.3.0",
    "@angular/platform-browser-dynamic": "^2.3.0",
    "@angular/router": "^3.3.0",
    "@angular/upgrade": "^2.3.0",
    "@types/googlemaps": "^3.25.37",
    "@types/hammerjs": "^2.0.33",
    "@types/node": "^6.0.46",
    "angular2-template-loader": "^0.6.0",
    "codelyzer": "^2.0.0-beta.3",
    "core-js": "^2.4.1",
    "hammerjs": "^2.0.8",
    "npm-check-updates": "^2.8.8",
    "npm-run-all": "^3.1.2",
    "raw-loader": "^0.5.1",
    "reflect-metadata": "^0.1.8",
    "rimraf": "^2.5.4",
    "rxjs": "^5.0.0-rc.5",
    "strip-loader": "^0.1.2",
    "systemjs": "0.19.41",
    "ts-loader": "^1.3.2",
    "tslint": "^4.0.2",
    "typescript": "^2.1.4",
    "webpack": "^1.14.0",
    "webpack-dev-server": "^1.16.2",
    "zone.js": "^0.7.2"
  }
}
